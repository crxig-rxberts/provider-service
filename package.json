{
  "name": "provider-service",
  "version": "1.0.0",
  "description": "Service for managing userType SERVICE_PROVIDER data",
  "main": "src/server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "lint": "eslint .",
    "docker:build": "docker build -t provider-service .",
    "docker:run": "docker run -p 3008:3008 -d provider-service",
    "docker:run:local": "docker run --name provider-service --network bookit-network -p 3004:3004 -e AWS_ENDPOINT=http://dynamodb-local:8000 -e AWS_REGION=local -d provider-service",
    "docker:stop": "docker stop provider-service",
    "docker:clean": "docker rm provider-service"
  },
  "dependencies": {
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@jest/globals": "^29.7.0",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "eslint": "^9.11.1",
    "eslint-plugin-jest": "^28.8.3",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0"
  }
}
